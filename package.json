  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec node -r @swc/register src/index.ts",
    "test": "nyc --reporter=text mocha test/**/*.test.ts -r @swc/register -extension ts",
    "unittest": "nyc --reporter=text mocha test/unit/*.test.ts -r @swc/register -extension ts",
    "intengration": "nyc --reporter=text mocha test/integration/*.test.ts -r @swc/register -extension ts",
    "endToEnd": "nyc --reporter=text mocha test/e2e/*.test.ts -r @swc/register -extension ts",
    "build": "swc src --out-dir dist"
  },
  "devDependencies": {
    "@swc/core": "^1.3.19",
    "@swc/register": "^0.1.10",
    "@tsconfig/recommended": "^1.0.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0-alpha.24",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.1.0",
    "mongodb-memory-server": "^8.10.1",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "supertest": "^6.3.1",
    "ts-mocha": "^10.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "commander": "^9.4.1",
    "conf": "^10.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.3",
    "morgan": "^1.10.0"
  },
  "description": "boilerplate tdd",
